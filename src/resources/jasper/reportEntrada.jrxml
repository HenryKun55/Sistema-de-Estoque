	<queryString>
		<![CDATA[SELECT
inputs.id_entrada as id_entrada,
inputs.created_at
FROM products
INNER JOIN inputs on products.id = inputs.product_id
WHERE inputs.created_at BETWEEN $P{Data_Inicio} AND $P{Data_Fim}
GROUP BY inputs.id_entrada]]>
	</queryString>
